{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#61AFEF",
    "green": "#98C379",
    "orange": "#D19A66",
    "red": "#E06C75",
    "yellow": "#E5C07B",
    "purple": "#C678DD",
    "cyan": "#56B6C2",
    "white": "#ABB2BF",
    "black": "#282C34"
  },
  "console_title_template": "{{ .Folder }}",
  "transient_prompt": {
    "template": "‚ùØ ",
    "foreground": "p:blue",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "foreground": "p:purple",
          "template": "{{ if .SSHSession }}Û∞£Ä {{ .UserName }}@{{ .HostName }} {{ end }}",
          "properties": {
            "display_host": true,
            "display_user": true
          }
        },
        {
          "type": "command",
          "style": "plain",
          "foreground": "p:cyan",
          "template": "{{ if .Output }}[{{ .Output }}] {{ end }}",
          "properties": {
            "shell": "bash",
            "command": "if git rev-parse --is-inside-work-tree >/dev/null 2>&1; then basename $(git rev-parse --show-toplevel); fi",
            "cache_duration": "5m"
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "p:blue",
          "template": " {{ .Path }} ",
          "properties": {
            "folder_icon": "",
            "folder_separator_icon": "/",
            "home_icon": "~",
            "style": "folder",
            "max_depth": 3,
            "cache_duration": "5m"
          }
        },
        {
          "type": "java",
          "style": "plain",
          "foreground": "p:orange",
          "template": "‚òï{{ .Major }} ",
          "properties": {
            "display_mode": "context",
            "cache_duration": "10m"
          }
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "p:green",
          "template": "‚¨¢{{ .Major }} ",
          "properties": {
            "display_mode": "context",
            "cache_duration": "10m"
          }
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:yellow",
          "template": "üêç{{ .Major }}.{{ .Minor }} ",
          "properties": {
            "display_mode": "context",
            "cache_duration": "10m"
          }
        },
        {
          "type": "command",
          "style": "plain",
          "foreground": "p:cyan",
          "template": "{{ if .Output }}ü©∞{{ .Output }} {{ end }}",
          "properties": {
            "shell": "bash",
            "command": "dir=\"$PWD\"; while [ \"$dir\" != \"/\" ]; do if [ -f \"$dir/Ballerina.toml\" ]; then if [ -f ~/.ballerina/ballerina-version ]; then cat ~/.ballerina/ballerina-version | sed 's/ballerina-//'; else echo 'bal'; fi; exit 0; fi; dir=$(dirname \"$dir\"); done",
            "cache_duration": "10m"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "p:green",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}"
          ],
          "template": " {{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}",
          "properties": {
            "branch_icon": "",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "cache_duration": "2m"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "p:yellow",
          "template": " {{ .FormattedMs }} ",
          "properties": {
            "threshold": 1000,
            "style": "austin"
          }
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground": "p:green",
          "foreground_templates": [
            "{{if lt .Percentage 20}}p:red{{end}}",
            "{{if and (ge .Percentage 20) (lt .Percentage 40)}}p:yellow{{end}}"
          ],
          "template": "{{ .Icon }}{{ .Percentage }}% ",
          "properties": {
            "charged_icon": "Û∞ÇÑ",
            "charging_icon": "Û∞ÇÑ",
            "discharging_icon": "Û∞Åπ",
            "cache_duration": "1m"
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "p:white",
          "properties": {
            "time_format": "15:04",
            "cache_duration": "10s"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:red{{ end }}",
            "{{ if eq .Code 0 }}p:blue{{ end }}"
          ],
          "template": "‚ùØ "
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
